# VITE_FIREBASE_MEASUREMENT_ID=tu-measurement-id

# Configuración adicional de Firebase para el sistema de pagos
FIREBASE_SERVICE_ACCOUNT_KEY=firebase-service-account.json

# Configuración de debug
DEBUG=false 